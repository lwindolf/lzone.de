                "webamp": "^1.5.0"
        },
        "devDependencies": {
                "@babel/preset-env": "^7.22.20",
                "babel-jest": "^29.7.0",
                "eslint": "^8.49.0",
                "fake-indexeddb": "^6.0.1",
                "jest": "^29.7.0",
                "jest-environment-jsdom": "^28.1.3",
                "whatwg-fetch": "^3.6.19"
